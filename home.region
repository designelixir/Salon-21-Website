<!doctype html>
<html style="background: var(--tan)">
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    {squarespace-headers}
  </head>
  <body class="flex-start-start" {.if collection.mainImage}style="background-image: url({collection.mainImage.assetUrl})"{.end}>
    <div class="flex-center-center flex-column" style="height: 100vh; width: 100vw; margin: 5%;">
        <img src="{website.socialLogoImageUrl}" style="width: 55%; max-width: 400px;"/>
        <div id="homepage-nav">
            <squarespace:navigation navigationId="homeNav" template="home-navigation" />
        </div>
    </div>
  

    
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    

  
</body>

</html>

<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    {squarespace-headers}
  </head>
  <squarespace:navigation navigationId="siteNav" template="site-navigation" />
  <body class="flex-start-start">
    {@|apply header.block}
    
    
    <div id="{squarespace.page-id}" class="{squarespace.page-classes} content-container" >

      <header class="contact-header flex-center-center">
        <div class=" flex-start-spacebetween" id="contact-container">
            <div id="contact-title">
                <h1>{collection.title}</h1>
            </div>
            <div class="contact-details">
                <p>Find us at <a href="https://www.google.com/maps/search/{addressLine1}{addressLine2}" target="_blank" >{.if website.location.addressTitle}{website.location.addressTitle}, {.end}{website.location.addressLine1}, {website.location.addressLine2}</a> by appointment only.</P
                <squarespace:block-field id="contactBlock" columns="12"/>
            </div>
        </div>
        
        
      </header>
      

      <div class="flex-center-center flex-column">
        <div role="main" data-content-field="main-content" class="flex-center-center flex-column">
        {squarespace.main-content}
        </div>
        
      </div>
      {@|apply footer.block}
      <!--Footer with open block field -->
      

    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    <!-- other scripts, and footer code injection -->
    {squarespace-footers}

  </div>
</body>

</html>

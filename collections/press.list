
<div id="pressList" class="flex-start-start flex-wrap">
<!-- item loop -->
{.repeated section items}
   <div class="press-frame" id={id} onclick="window.open('{clickthroughUrl}')" style="background-image: url({assetUrl}); background-size: cover;">
      
      
      <div class="press-info">
        <div class="flex-center-center">
          <img src="{sourceUrl}" style="width: 100px"/>
        </div>
        <p style="text-align: center;"><i>{body|truncate 200}</i></p>
      </div>
   </div>
{.end}
<script>

var bodyWidth = $(window).width()
console.log(bodyWidth)
$('.press-frame').css({'height': bodyWidth / 4 + 'px', 'width': bodyWidth / 4 + 'px'})

</script>
</div>

